{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint src/**/*.js server.js",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.production.js",
    "postbuild": "copyfiles index.html dist",
    "old_start": "webpack-dev-server",
    "old_lint": "eslint src",
    "old_prebuild": "rimraf dist",
    "old_build": "cross-env NODE_ENV=production webpack -p --config webpack.config.production.js",
    "old_postbuild": "copyfiles index.html dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "author": "Erik Hellman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.22.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^4.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.0",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-eslint": "^7.2.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "express": "^4.15.2",
    "file-loader": "^0.11.0",
    "history": "^4.6.1",
    "json-loader": "^0.5.4",
    "preact": "^7.2.1",
    "preact-compat": "^3.14.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.16.1",
    "styletron": "2.4.0",
    "styletron-client": "2.4.0",
    "styletron-react": "2.4.0",
    "url-loader": "^0.5.8"
  }
}
